{
    "name": "miguelpedregosa/huruk",
    "description": "Micro http framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Pedregosa",
            "email": "miguelpedregosa@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "symfony/routing": "2.*",
        "symfony/config": "2.*",
        "symfony/http-foundation": "2.*",
        "symfony/event-dispatcher": "2.*",
        "psr/log": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpunit/phpunit-mock-objects": "2.*",
        "phpunit/phpunit-story": "dev-master",
        "mockery/mockery": "dev-master",
        "phpspec/phpspec": "2.0.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Huruk\\": "src/"
        }
    }
}
